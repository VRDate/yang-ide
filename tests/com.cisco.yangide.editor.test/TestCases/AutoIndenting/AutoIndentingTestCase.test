get-preferences-menu | click
with [get-window Preferences] {
    get-tree | select "YANG/Syntax Coloring"
    get-link "Default colors and font can be configured on the <a href=\"org.eclipse.ui.preferencePages.GeneralTextEditor\">\'Text Editors\'</a> and on the <a href=\"org.eclipse.ui.preferencePages.ColorsAndFonts\">\'Colors and Fonts\'</a> preference page." 
        | click-link "org.eclipse.ui.preferencePages.GeneralTextEditor"
    get-button "Insert spaces for tabs" | check
    get-button OK | click
}
with [get-view "Package Explorer" | get-tree] {
    select "model-parent/model-flow-statistics/src/main/yang/opendaylight-flow-statistics.yang" | click -default
    select "model-parent/model-flow-statistics/src/main/yang/opendaylight-flow-statistics.yang" | double-click
}
with [get-editor "opendaylight-flow-statistics.yang" | get-text-viewer] {
    set-caret-pos 1 38
    key-type Del -times 5
    key-type Enter
}
get-editor "opendaylight-flow-statistics.yang" | get-text-viewer | set-caret-pos 3 21
get-editor "opendaylight-flow-statistics.yang" | get-text-viewer | get-property "styles[6].start" | equals 42 
    | verify-true
